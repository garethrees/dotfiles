<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>uk.co.garethrees.clear-desktop-downloads</string>
  <key>ProgramArguments</key>
  <array>
    <string>sh</string>
    <string>-c</string>
    <string>/Users/gareth/bin/doclinks && mv /Users/gareth/Downloads/* /Users/gareth/doc/previous/ && mv /Users/gareth/Desktop/* /Users/gareth/doc/previous/</string>
  </array>
  <key>StartCalendarInterval</key>
  <array>
    <dict>
      <key>Minute</key>
      <integer>0</integer>
    </dict>
    <dict>
      <key>Hour</key>
      <integer>0</integer>
    </dict>
    <dict>
      <key>Month</key>
      <integer>1</integer>
    </dict>
  </array>
</dict>
</plist>
